{
  "presets": [
    [
      "module:metro-react-native-babel-preset"
    ]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": [
          "./src"
        ],
        "extensions": [
          ".js",
          ".ios.js",
          ".android.js"
        ],
        "alias": {
          "test": "./test",
          "underscore": "lodash",
          "@actions": "./src/actions",
          "@assets": "./src/assets",
          "@common": "./src/common",
          "@components": "./src/components",
          "@componentsCommon": "./src/components/common",
          "@constants": "./src/constants",
          "@navigators": "./src/navigators",
          "@reducers": "./src/reducers",
          "@screens": "./src/screens",
          "@store": "./src/store",
          "@utils": "./src/utils",
          "@srcState": "./src/state",
          "@stateRedux": "./src/state/redux",
          "@stateUtils": "./src/state/utils"
        }
      }
    ],
    [
      "@babel/plugin-transform-runtime"
    ]
    // [
    //   "transform-remove-console"
    // ]
  ],
  "env": {
    "production": {
      // "plugins": [
      //   "transform-remove-console"
      // ]
    }
  }
}